curl "http://localhost:8080/user/search?userName=one&userEmail=one@gmail.com"                                      ^C

curl -X POST http://localhost:8080/user/add -H "Content-Type: application/json" -d  '{"userName": "two", "userEmail": "two@gmail.com"}'

curl http://localhost:8080/user/getAll\?page=1\&size=1
curl http://localhost:8080/user/getAll\?page=1\&size=1\&direction=asc

curl http://localhost:8080/user/searchPage\?page\=0\&size\=4\&name\=one
